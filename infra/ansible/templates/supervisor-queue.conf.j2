[program:laravel-queue]
directory={{ app_root }}
command=/usr/bin/php artisan queue:work {{ queue_conn }} --sleep=3 --tries=3 --max-time=3600
autostart=true
autorestart=true
user={{ app_user }}
redirect_stderr=true
stdout_logfile={{ app_root }}/storage/logs/queue-worker.log
stopasgroup=true
killasgroup=true